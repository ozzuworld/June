# June Services Environment Configuration
# Copy this to .env and fill in your values

# LiveKit Configuration (REQUIRED)
LIVEKIT_WS_URL=wss://livekit.ozzu.world
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret

# Orchestrator Configuration
ORCHESTRATOR_URL=https://api.ozzu.world

# Model Configuration
COSYVOICE_MODEL=CosyVoice2-0.5B

# STT Timing Configuration (Optional - customize as needed)
MAX_UTTERANCE_SEC=15.0
MIN_UTTERANCE_SEC=1.0
SILENCE_TIMEOUT_SEC=2.5

# TTS Voice Configuration (Optional)
DEFAULT_VOICE=female-calm
VOICE_SPEED=1.0